<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbMainContainer" styleClass="constants" stylesheets="@../../css/calculator-sandy.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.normic.settings.ConstantsController">
   <children>
      <TableView fx:id="tableView" prefHeight="400.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tcName" prefWidth="150.0" text="Наименование" />
          <TableColumn fx:id="tcNormType" maxWidth="80.0" minWidth="80.0" resizable="false" text="Цех" />
            <TableColumn fx:id="tcJobType" minWidth="80.0" resizable="false" text="Участок МК" />
            <TableColumn fx:id="tcMeasurement" minWidth="40.0" prefWidth="40.0" resizable="false" text="Ед.изм." />
            <TableColumn fx:id="tcNormTime" minWidth="40.0" prefWidth="40.0" resizable="false" text="Норма на ед." />
            <TableColumn fx:id="tcDescription" minWidth="200.0" prefWidth="200.0" text="Описание" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
