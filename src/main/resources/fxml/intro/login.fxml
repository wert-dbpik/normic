<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.normic.controllers.intro.LoginController">
   <children>
      <Label text="Пользователь" />
      <ComboBox fx:id="cbxUser" prefWidth="300.0" />
      <Label text="Пароль" />
      <TextField fx:id="tfPass" />
      <VBox alignment="CENTER">
         <children>
            <Button fx:id="btnOK" mnemonicParsing="false" text="OK">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
