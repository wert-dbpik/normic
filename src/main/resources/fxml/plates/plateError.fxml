<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbOperation" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.normic.controllers.PlateErrorController">
    <children>
        <HBox alignment="CENTER_LEFT" spacing="10.0">
            <children>
                <ImageView fx:id="ivOperation" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../pics/btns/operation.png" />
                    </image>
                </ImageView>
                <Label fx:id="lblOperationName" text="ERROR" textFill="#2f23d0">
                    <font>
                        <Font name="System Bold Italic" size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                    </VBox.margin>
                </Label>
                <HBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <ImageView fx:id="ivDeleteOperation" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../pics/btns/close.png" />
                            </image>
                        </ImageView>
                    </children>
                </HBox>
            </children>
        </HBox>
      <Separator prefWidth="200.0" />
        <HBox spacing="5.0">
         <children>
            <Label text="Ошибка конвертации операции: ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lbErrorOpData" lineSpacing="5.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
    </padding>
</VBox>
