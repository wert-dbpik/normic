<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.normic.controllers.PlateLevelingSealerController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label fx:id="lblOperationName" text="НАНЕСЕНИЕ НАЛИВНОГО УПЛОТНИТЕЛЯ" textFill="#2f23d0">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <ImageView fx:id="ivHelpOnLevelingSealer" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../pics/btns/help.png" />
               </image>
            </ImageView>
            <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <ImageView fx:id="ivDeleteOperation" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../pics/btns/close.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="10.0" />
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <VBox spacing="5.0">
                           <children>
                              <Label text="W пр., мм">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                              <ComboBox fx:id="cmbxSealerWidth" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="70.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity" spacing="5.0">
                           <children>
                              <Label text="А, мм">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="tfA" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="80.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity" spacing="5.0">
                           <children>
                              <Label text="В, мм">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="tfB" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="80.0" />
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets right="5.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
            <Separator orientation="VERTICAL" />
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <VBox alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity" spacing="5.0">
                           <children>
                              <Label text="Комп А, кг">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="tfCompA" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="70.0" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity" spacing="5.0">
                           <children>
                              <Label text="Комп Б, кг">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="tfCompB" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="70.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Label text="T упл : ">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfNormTime" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" />
            <Label fx:id="lblNormTimeMeasure" text="мин">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Separator maxWidth="1.7976931348623157E308" />
   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
